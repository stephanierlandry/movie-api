in this view will be loaded\n    // if (!user) return <LoginView onLoggedIn={user => this.onLoggedIn(user)} />;\n\n    // if (!user) return <RegistrationView onLoggedIn={user => this.onLoggedIn(user)} />;\n\n\n    return(\n      <div className=\"main-view\">\n        {selectedMovie ?\n          <Container>\n            <Row>\n              <MovieView movie={selectedMovie}/>\n            </Row>\n          </Container>\n          :\n          <Container>\n            <Row>\n            { movies ?\n              movies.map(movie => (\n                <Col md={4}>\n                  <MovieCard key={`${movie._id}-${Math.random()}`} movie={movie} onClick={movie => this.onMovieClick(movie)}/>\n                </Col>\n              ))\n              :\n              <React.Fragment>loading...</React.Fragment>\n            }\n            </Row>\n          </Container>\n        }\n      </div>\n    )\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"5c426de14c26b9febfcde9e88b2e6d03","cacheData":{"env":{}}}